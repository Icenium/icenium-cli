    "preuninstall": "node preuninstall.js",
    "prepublish": "node prepublish.js"
  },
  "main": "./lib/appbuilder-cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Icenium/icenium-cli.git"
